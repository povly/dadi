        .container.margin-center
            .catalog-cons__item.relative
                .catalog-cons__img-left.absolute.bottom-0.left-0.p-img
                    +picture('img/catalogs/cons/left.png', 'img/catalogs/cons/left', true, '', 991, 653, '', '')
